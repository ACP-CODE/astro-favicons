{
  "name": "astro-favicons",
  "version": "3.0.0",
  "description": "An all-in-one favicon and PWA assets generator for Astro projects. It automates the creation of favicons, manifest, and supports hot reloading for efficient development.",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.ts"
    },
    "./middleware": {
      "import": "./src/middleware/index.js"
    }
  },
  "files": [
    "dist/index.d.ts",
    "dist/index.mjs",
    "dist/middleware.d.ts",
    "dist/middleware.mjs"
  ],
  "scripts": {
    "fmt": "prettier --write .",
    "build": "unbuild",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ACP-CODE/astro-favicons.git"
  },
  "keywords": [
    "astro",
    "astro-integration",
    "favicons",
    "web-manifest",
    "apple-icons",
    "apple-startup",
    "Capo.js",
    "pwa",
    "manifest.webmanifest",
    "browserconfig.xml",
    "yandex-browser-manifest.json",
    "android",
    "android-chrome",
    "windows",
    "windows-tile",
    "yandex"
  ],
  "author": "Junlin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ACP-CODE/astro-favicons/issues"
  },
  "homepage": "https://github.com/ACP-CODE/astro-favicons#readme",
  "dependencies": {
    "favicons": "../../libraries/favicons"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "astro": "4.14.0",
    "prettier": "^3.3.3",
    "unbuild": "^2.0.0"
  },
  "peerDependencies": {
    "astro": ">= 4.0.0"
  }
}
