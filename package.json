{
  "name": "astro-favicons",
  "version": "2.1.6",
  "description": "A Multi-platform, All Browser Favicon generator for Astro Project.",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist/index.d.ts",
    "dist/index.mjs"
  ],
  "keywords": [
    "withastro",
    "favicon.ico",
    "favicons",
    "favicons generator",
    "Android",
    "Apple Icon",
    "Apple Startup",
    "Windows Tile",
    "PWA",
    "manifest.webmanifest",
    "yandex-browser-manifest.json",
    "browserconfig.xml"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ACP-CODE/astro-favicons.git"
  },
  "author": "Junlin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ACP-CODE/astro-favicons/issues"
  },
  "homepage": "https://github.com/ACP-CODE/astro-favicons#readme",
  "devDependencies": {
    "@types/node": "^22.7.9",
    "astro": "^4.16.7",
    "unbuild": "^2.0.0"
  },
  "peerDependencies": {
    "astro": "^4.0.0"
  },
  "dependencies": {
    "favicons-lib": "^7.2.1"
  }
}